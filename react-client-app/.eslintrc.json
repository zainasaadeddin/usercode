{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "react/forbid-prop-types": "off",
        "no-case-declarations": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
